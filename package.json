{
  "name": "10-money-vue",
  "version": "1.0.0",
  "description": "reworking the money app",
  "main": "index.js",
  "scripts": {
   "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
   "start:prod": "node index.js",
   "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
   "client": "cd client && npm run start",
   "seed": "node scripts/index.js",
   "install": "cd client && npm install",
   "build": "cd client && npm run build",
   "heroku-postbuild": "npm run build"
  },
  "author": "Yours Truly",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  }
}
